<div class="panel-heading subcategory-header subcategory <%= positive ? 'credit' : 'debit' %>">
  <div class="panel-icon">
    <a role="button" data-collapse="#panel<%= "#{category.id}#{positive ? 1 : 2}" %>" title="collapse">
      <%= fa_icon "caret-down" %>
    </a>
  </div><!-- /panel-actions -->
  <div class="panel-actions child-category">
    <h6 class="money-value">$ 1.700,00</h6>
  </div>
  <h4 class="panel-title"><%= category.name %></h4>
</div><!-- /panel-heading -->
<div class="panel-body category-body" style="display: block;">
  <% entry_details.each do |entry_detail| %>
      <%= render partial: 'financial_control/entry', locals: {entry_detail: entry_detail} %>
  <% end %>
</div><!-- /panel-body -->